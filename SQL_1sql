SELECT *
FROM EMPLOYEE
WHERE SUPERSSN IS NULL
ORDER BY FNAME ;
--ORDER BY SALARY;

INSERT INTO EMPLOYEE VALUES ('JOEY', 'F', 'TRIBBIANI', 123123456, TO_DATE('09-AUG-1980','DD-MON-YYYY'), 'APT 12, MANHATTAN, TX', 'M' , 60000, '123456789', 3);

UPDATE EMPLOYEE
SET SUPERSSN = NULL
WHERE SSN = 123123456;

DESCRIBE EMPLOYEE;